<html>
<head>

  <!-- CSS -->
  <link type="text/css" href="./js/jquery-ui/css/smoothness/jquery-ui-1.8.13.custom.css" rel="stylesheet" />
  <link type="text/css" href="./js/jquery-ui-ribbon/css/ui.button.css" rel="stylesheet" />
  <link type="text/css" href="./js/jquery-ui-ribbon/css/ui.ribbon.css" rel="stylesheet" />

  <link type="text/css" href="./css/viewport.css" rel="stylesheet" />
  <link type="text/css" href="./css/ribbon.css" rel="stylesheet" />


  <link type="text/css" href="/js/jquery.svg/jquery.svg.css" rel="stylesheet" /> 

  <!-- JS -->
  <script src="./js/jquery-1.6.1.min.js" type="text/javascript"></script>

  <script type="text/javascript" src="/js/jquery.svg/jquery.svg.js"></script>
  <script type="text/javascript" src="/js/jquery.svg/jquery.svgdom.js"></script>

  <script type="text/javascript" src="/js/raphael-min.js"></script>


  <script language="JavaScript" type="text/javascript" src="./js/jquery-ui-ribbon/js/ui.core.js"></script>
  <script language="JavaScript" type="text/javascript" src="./js/jquery-ui-ribbon/js/ui.tabs.js"></script>
  <script language="JavaScript" type="text/javascript" src="./js/jquery-ui-ribbon/js/ui.ribbon.js"></script>
  <script language="JavaScript" type="text/javascript" src="./js/jquery-ui-ribbon/js/ui.button.js"></script>

  <script language="JavaScript" type="text/javascript" src="/js/jquery.corner.js"></script>

  <% $coffee_scripts.each do |script| %>
  <script language="JavaScript" type="text/javascript" src="/js/<%=script%>.js"></script>
  <% end %>

</head>

<body>


<!--
  The Toolbars
-->

<div id="ribbon">
	<ul>
	  <li class="ribbon-title">New CAD</li>
<!--		<li><a href="#ribbonTabFeatures"><span><label>Features</label></span></a></li>-->
		<li><a href="#ribbonTabSketch"><span><label>Sketch</label></span></a></li>
	</ul>
<!--	<div id="ribbonTabFeatures">
		<ul>
			<li>
				<div>
					<a>Extrude</a>
			  </div>
      </li>
    </ul>
	</div> -->
	<div id="ribbonTabSketch">
		<ul>
		  <% $commands.each do |cmd_set| %>
		    <% cmd_set[1].each do |section| %>
			    <li class="<%=cmd_set[0].downcase%>-<%= css_name(section[0]) %>-section">
				    <div>
				      <% section[1].each do |cmd| %>
					      <span class="cmd <%= cmd[1][:css_name] %>-button" cmd="<%= cmd[1][:css_name] %>"><%= cmd[0] %></span>
					    <% end %>
			      </div>
          </li>
        <% end %>
      <% end %>
    </ul>
	</div>
</div>


<!--
  The SVG Drawing Surface
-->

<div class="viewport">
<!--<svg class="sketch-plain" xmlns="http://www.w3.org/2000/svg">
  <g class="viewport-transform">
      <rect width="100%" height="20" fill="orange" />
      <rect id="redrect" width="300" height="100" fill="red" />
      <rect x="0" y="0" width="60" height="10"/>
  </g>
</svg>
-->
</div>

</body>
</html>
